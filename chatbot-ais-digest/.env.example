# Example environment file for local development
# Copy this file to .env and fill in your actual values

# Telegram bot token for local testing
TELEGRAM_BOT_TOKEN_AIS_Digest_local=your_local_bot_token_here

# API Keys
OPENAI_API_KEY=your_openai_api_key_here
GOOGLE_API_KEY=your_google_api_key_here
RAPIDAPI_KEY=your_rapidapi_key_here

# S3 Bucket for storing messages
S3_BUCKET=your_s3_bucket_name_here

# Comma-separated list of allowed user IDs
USERS_ALLOWED=user_id1,user_id2

# Optional: Auxiliary username for special features
AUX_USERNAME=auxiliary_username_here