# Example environment file for local development
# Copy this file to .env and fill in your actual values

# Telegram bot token for local testing
TELEGRAM_BOT_TOKEN_AIS_Digest_local=your_local_bot_token_here

# API Keys
OPENAI_API_KEY=your_openai_api_key_here
GOOGLE_API_KEY=your_google_api_key_here
RAPIDAPI_KEY=your_rapidapi_key_here

MODEL_PROVIDER=gemini
GEMINI_MODEL=gemini-2.5-pro
OPENAI_MODEL=gpt-4o-mini

# S3 Bucket for storing messages
S3_BUCKET=your_s3_bucket_name_here

# Comma-separated list of allowed user IDs
USERS_ALLOWED=user_id1,user_id2

# Optional: Auxiliary username for special features
AUX_USERNAME=auxiliary_username_here

AWS_ACCESS_KEY_ID=your_aws_access_key_id_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key_here
AWS_DEFAULT_REGION=your_aws_default_region_here

MAILGUN_API_KEY=your_mailgun_api_key_here
MAILGUN_DOMAIN=your_mailgun_domain_here